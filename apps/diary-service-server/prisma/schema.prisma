datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model DiaryEntry {
  content   String?
  createdAt DateTime  @default(now())
  date      DateTime?
  id        String    @id @default(cuid())
  title     String?
  updatedAt DateTime  @updatedAt
}
